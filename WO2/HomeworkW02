//  1 to 100, but here's the catch, multiple of three should print "Fizz" and similarly print "Buzz" for multiples of 5 and lastly print "FizzBuzz" for multiples of three and five.
for (var i = 0; i < 100; i++) {
  if (i % 3 && i % 5) {
    console.log("FizzBuzz");
  } else {
    if (i % 3 == 0) {
      console.log("Fizz");
    }
    if (i % 5 == 0) {
      console.log("Buzz");
    }
  }
